        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tagger"
        mc:Ignorable="d"
        Title="Tag Manager" Loaded="Window_Loaded" SizeToContent="WidthAndHeight" WindowStartupLocation="Manual" ShowInTaskbar="False" MaxHeight="800" KeyUp="Window_KeyUp">
    <Grid>
        <ScrollViewer x:Name="Scroller" VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="TagList" Height="Auto" Width="Auto">
                
            </StackPanel>
        </ScrollViewer>
        
    </Grid>
</Window>
